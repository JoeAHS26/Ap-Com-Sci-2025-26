// page 14
let orangeCrates  = 14
let melonCrates = 3
let orangePalletWeight = 100
let melonPalletWeight = 200
let orangeWeight = orangeCrates * orangePalletWeight
let melonWeight = melonCrates * melonPalletWeight
let totalWeight = orangeWeight + melonWeight
let es = totalWeight / 2
let leftSideOfTruck = es / orangePalletWeight
let RightSideOfTruck = orangeCrates - leftSideOfTruck
